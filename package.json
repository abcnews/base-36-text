{
  "name": "@abcnews/base-36-text",
  "version": "1.0.0",
  "description": "Tools for encoding / decoding text to / from base36 strings",
  "license": "MIT",
  "author": "Australian Broadcasting Corporation",
  "contributors": [
    "Colin Gourlay <Gourlay.Colin@abc.net.au>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abcnews/base-36-text.git"
  },
  "bugs": {
    "url": "https://github.com/abcnews/base-36-text/issues"
  },
  "bin": {
    "b36t": "bin/index.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "start": "tsup src/index.ts --dts --sourcemap --format esm,cjs --watch",
    "build": "tsup src/index.ts --dts --sourcemap --format esm,cjs",
    "test": "jest",
    "lint": "eslint src",
    "size": "size-limit",
    "release": "np",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "example": "npm run build && rollup -c"
  },
  "publishConfig": {
    "access": "public"
  },
  "browserslist": "> 0.25%, not dead",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.4",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "husky": "^6.0.0",
    "install": "^0.13.0",
    "jest": "^29.3.1",
    "np": "^7.6.2",
    "rollup": "^3.7.0",
    "rollup-plugin-serve": "^2.0.2",
    "size-limit": "^8.1.0",
    "ts-jest": "^29.0.3",
    "tslib": "^2.0.3",
    "tsup": "^6.5.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "base-x": "^4.0.0",
    "core-js": "^3.7.0",
    "sade": "^1.7.4"
  }
}
